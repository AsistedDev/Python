binary = int(input("Input you binary code: "))
indexes = []
factorials = []

nums = []
total = 0

for i, v in enumerate(str(binary)):
    indexes.append(v)
    factorials.append(i)

factorials.reverse()

for c, index in enumerate(indexes):
    nums.append(int(index) * (2**factorials[c]))

for _, number in enumerate(nums):
    total += number

print(total, chr(total))
